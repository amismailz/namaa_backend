{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "arbermustafa/filament-locationpickr-field": "^2.0",
        "amidesfahani/filament-tinyeditor": "^2.1",
        "bensampo/laravel-enum": "^6.11",
        "bezhansalleh/filament-shield": "^3.3",
        "eightynine/filament-advanced-widgets": "^3.0",
        "filament/actions": "^3.3",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.3",
        "intervention/image": "^3.10",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^11.9",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "maestroerror/php-heic-to-jpg": "^1.0",
        "mailjet/laravel-mailjet": "^3.1",
        "mailjet/mailjet-apiv3-php": "^1.6",
        "moyasar/moyasar": "^1.1",
        "pbmedia/laravel-ffmpeg": "^8.7",
        "predis/predis": "^3.0",
        "pusher/pusher-php-server": "^7.2",
        "pxlrbt/filament-excel": "^2.4",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-medialibrary": "^11.13",
        "spatie/laravel-translatable": "^6.11",
        "wotz/laravel-swagger-ui": "^1.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^3.5",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
